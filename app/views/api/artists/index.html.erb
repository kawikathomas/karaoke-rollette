<h1>Artists#index</h1>
<p>Find me in app/views/artists/index.html.erb</p>

<h1 class="page-header">Artists Matching <%= params[:artist_name] %></h1>
<ul>
 <% @artists.each do |artist| %>
   <% @artist = artist %>
   <li><%= @artist.name %> | <%= link_to "Show", api_artist_path(@artist.id) %></li>
 <% end %>
</ul>
